{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "noEmit": true,
        "module": "ESNext",
        "jsx": "preserve",
        "jsxFactory": "PyDefUtils.h",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        // "noImplicitAny": false,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "allowUmdGlobalAccess": true,
        "sourceMap": true
    },
    "include": [
        "./",
    ],
    "exclude": [
        "node_modules",
        "./PyDefUtils.js"
    ]
}